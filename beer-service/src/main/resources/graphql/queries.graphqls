type Query {
hello: String
}
type Mutation {
    addBeer(beerDto:BeerIn!): BeerOut
    updateBeer(beerId:String!, beerDto:BeerIn!): BeerOut
}

input BeerIn {
    name: String
    upc: String
    price: String
    style: BeerStyle
}

type BeerOut {
    id: ID
    name: String
    upc: String
    price: String
    style: BeerStyle
}

enum BeerStyle {
    LAGER, PILSNER, STOUT, GOSE, PORTER, ALE, WHEAT, IPA, PALE_ALE, SAISON
}